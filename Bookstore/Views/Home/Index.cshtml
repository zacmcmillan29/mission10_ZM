@{
    ViewBag.Title = "Home Page";
}
@model BooksViewModel


<h1>Welcome to the Bookstore</h1>
@foreach (Book b in Model.Books)
{
    <partial name="BookSummary" model="b" />
}

<div page-stuff="@Model.PageInformation" page-action="Index" page-class="btn" 
     page-class-normal="btn-outline-dark" page-class-enabled="true" page-class-selected="btn-primary" class="btn-group pull-right m-1">></div>

