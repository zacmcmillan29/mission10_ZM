@{
}
@model IEnumerable<string>

<div class="">
    <a asp-controller="Home" asp-action="Index" asp-route-category="" asp-route-pageNum="1" class="btn btn-block btn-outline-secondary">Home</a>

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-category="@t" asp-route-pageNum="1"
           class="btn btn-block @(ViewBag.SelectedType == t ? "btn-primary" :  "btn-outline-secondary")">@t</a>//this loops through and display
    }
</div>